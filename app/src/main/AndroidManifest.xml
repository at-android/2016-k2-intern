<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.asiantech.training">
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <activity android:name=".activities.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".activities.AddAlarmActivity"/>
    <activity android:name=".activities.CaculatorActivity"/>
    <activity android:name=".activities.DeleteAlarmActivity"/>
    <activity android:name=".activities.EditAlarmActivity"/>
    <service
        android:name=".services.AlarmServices"
        android:enabled="true" />
    <receiver android:name=".broadcast.AlarmReceiver" />
    </application>
</manifest>